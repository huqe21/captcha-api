{
    "manifest_version": 3,
    "name": "Valid8",
    "version": "1.0",
    "description": "Sendet CAPTCHA zur LÃ¶sung an Valid8",
    "permissions": ["activeTab", "http://127.0.0.1:5000/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }
  